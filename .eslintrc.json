{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": ["error", "always", {
            "js": "never'"
          }],
        "no-param-reassign": "off",
        "no-lonely-if": "off",
        "comma-dangle": ["error", "never"]
    }
}
